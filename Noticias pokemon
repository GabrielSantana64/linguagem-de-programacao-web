    <?php
        // Contador de visitas
        $contador_file = 'contador.txt';
        if (file_exists($contador_file)) {
            $contador = file_get_contents($contador_file);
            $contador = intval($contador);
            $contador++;
        } else {
            $contador = 1;
        }
        file_put_contents($contador_file, $contador);
        echo "<p>Visitas: $contador</p>";
    ?>

    <script>
        function mostrarPokemon() {
            var pokemonImg = document.getElementById('pokemon-img');
            // Simula a troca de imagens para demonstração
            pokemonImg.src = 'pokemon_do_dia.jpg';
        }
    </script>
