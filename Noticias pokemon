        <h2>Notícias Gerais</h2>
        <?php
            // Função para ler e exibir notícias
            function exibir_noticias($arquivo) {
                if (file_exists($arquivo)) {
                    $noticias = file($arquivo, FILE_IGNORE_NEW_LINES | FILE_SKIP_EMPTY_LINES);
                    foreach ($noticias as $noticia) {
                        $dados = explode("###", $noticia);
                        $titulo = htmlspecialchars($dados[0]);
                        $texto = htmlspecialchars($dados[1]);
                        $imagem = htmlspecialchars($dados[2]);
                        echo "<article>";
                        echo "<img src='$imagem' alt='Imagem Notícia'>";
                        echo "<h3>$titulo</h3>";
                        echo "<p>$texto</p>";
                        echo "</article>";
                    }
                } else {
                    echo "<p>Não há notícias disponíveis.</p>";
                }
            }
            
            // Adicionar notícia se o formulário foi submetido
            if ($_SERVER["REQUEST_METHOD"] == "POST" && isset($_POST['submit'])) {
                $titulo = htmlspecialchars($_POST['titulo']);
                $texto = htmlspecialchars($_POST['texto']);
                $url_imagem = htmlspecialchars($_POST['url_imagem']);
                
                // Adicionar notícia ao arquivo
                $noticias_file = 'noticias.txt';
                $nova_noticia = "$titulo###$texto###$url_imagem\n";
                file_put_contents($noticias_file, $nova_noticia, FILE_APPEND | LOCK_EX);
                echo "<p>Notícia adicionada com sucesso!</p>";
            }
        ?>
        <!-- Formulário para adicionar notícias -->
        <section id="adicionar">
            <h2>Adicionar Notícia</h2>
            <form action="index.php#adicionar" method="post">
                <label for="titulo">Título:</label>
                <input type="text" id="titulo" name="titulo" required><br><br>
                <label for="texto">Texto da Notícia:</label><br>
                <textarea id="texto" name="texto" rows="4" cols="50" required></textarea><br><br>
                <label for="url_imagem">URL da Imagem:</label>
                <input type="url" id="url_imagem" name="url_imagem" required><br><br>
                <button type="submit" name="submit">Adicionar Notícia</button>
            </form>
        </section>
        
        <!-- Exibir notícias existentes -->
        <?php exibir_noticias('noticias.txt'); ?>
    </section>

